input OrderProductReq {
    productId: String,
    productName: String,
    qty: Int,
    price: Int
}

input OrderReq {
    orderProducts: [OrderProductReq]
    customerName: String
}

type OrderProduct {
    productId: String,
    productName: String,
    qty: Int,
    price: Int
}

type OrderData {
    orderProducts: [OrderProduct]
}

type Order {
    id: String,
    orderData: OrderData,
    createdDate: String
    createdBy: String
}