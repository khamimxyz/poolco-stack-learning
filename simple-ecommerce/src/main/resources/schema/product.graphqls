input ProductReq {
    name: String,
    category: String,
    price: Int
}

input SortReq {
    sortBy: String!
    sortType: String!
}

input FilterProductReq {
    category: String,
    sortReqList: [SortReq]
}

type Product {
    id: String,
    name: String,
    category: String
    price: Int
    createdDate: String
}

type ProductWithReview {
    product: Product,
    reviews: [Review]
}